{
  "name": "laravel_grid",
  "version": "3.0.0",
  "description": "Laravel data grid inspired by the yii2 gridview. Based on the randomwhitetrash/laravel-grid package.",
  "main": "none",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "babel --presets es2015 src/resources/assets/js/grid.js -o src/resources/assets/dist/js/grid.js --source-maps",
    "css": "node-sass --include-path scss src/resources/assets/css/grid.scss src/resources/assets/dist/css/grid.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randomwhitetrash/laravel-grid.git"
  },
  "keywords": [
    "laravel",
    "laravel-grid",
    "grid",
    "grid-generator",
    "pjax",
    "bootstrap"
  ],
  "author": "bmimage@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RandomWhiteTrash/laravel-grid/issues"
  },
  "homepage": "https://github.com/RandomWhiteTrash/laravel-grid#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.9.0"
  }
}
