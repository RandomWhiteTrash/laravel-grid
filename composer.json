{
  "name": "randomwhitetrash/laravel-grid",
  "description": "A grid view for laravel, inspired by the yii2 grid widget. Based on the leantony/laravel-grid package.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "leantony",
    "random-white-trash",
    "bootstrap",
    "laravel-grid",
    "laravel",
    "pjax",
    "grid",
    "laravel10"
  ],
  "homepage": "https://github.com/RandomWhiteTrash/laravel-grid",
  "authors": [
    {
      "name": "Bruno Michalski",
      "email": "bmimage@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/view": "~10",
    "illuminate/support": "~10",
    "illuminate/console": "~10",
    "illuminate/contracts": "~10",
    "illuminate/http": "~10",
    "illuminate/database": "~10",
    "illuminate/pagination": "~10"
  },
  "suggest": {
    "maatwebsite/excel": "For exporting grid data as excel (~3.0)",
    "barryvdh/laravel-dompdf": "For exporting grid data as PDF (^0.8.2)"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "~3.0",
    "orchestra/testbench-dusk": "^3.5",
    "phpunit/php-code-coverage": "^7.0.5",
    "barryvdh/laravel-dompdf": "^0.8.2",
    "maatwebsite/excel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "RandomWhiteTrash\\Grid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RandomWhiteTrash\\Grid\\Providers\\GridServiceProvider"
      ],
      "aliases": {
        "Modal": "RandomWhiteTrash\\Grid\\Facades\\Modal"
      }
    },
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
